<div class="card-trip rounded mt-4">
  <%= cl_image_tag venue.photos[0].key %>
  <div class="position-abso">
      <%= link_to 'Remove',  venue_delete_night_venue_night_path(@night,venue) %>
  </div>
  <div class="card-trip-infos">
    <div>
      <h3 class="mb-2"><%= venue.name %></h3>
      <p class="ct-text-light"><%= venue.venue_type %> / <%= venue.category %></p>
      <p><span class="ct-text-12"><%= venue.address %></span></p>
    </div>
    <h5 class="card-trip-pricing"><%= venue.price_segment %></h5>
  </div>
</div>
